{
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "strict": 1,
    "no-underscore-dangle": 0,
    "quotes": [
      0,
      "single"
    ],
    "no-trailing-spaces": 2,
    "no-debugger": 0,
    "eol-last": 0
  }
}
